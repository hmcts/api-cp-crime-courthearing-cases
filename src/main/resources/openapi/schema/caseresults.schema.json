{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://justice.gov.uk/CaseJudiciaryResults.schema.json",
  "title": "CaseJudiciaryResults",
  "type": "array",
  "items": {
    "title": "CaseJudiciaryResult",
    "description": "Case-level Judiciary Result",
    "type": "object",
    "properties": {
      "resultText": {
        "description": "Result",
        "title": "Judiciary Result",
        "type": "string",
        "default": "",
        "examples": [
          "This is the example outcome of case results"
        ]
      },
      "caseUrn": {
        "description": "Case Unique Reference Number",
        "title": "Case Urn",
        "type": "string",
        "default": "",
        "examples": [
          "This is the example of case urn"
        ]
      },
      "hearingId": {
        "description": "Hearing Id",
        "title": "Hearing Id",
        "type": "string",
        "default": "",
        "examples": [
          "This is the example of case hearing id"
        ]
      }
    },
    "required": ["resultText"],
    "additionalProperties": false
  }
}
